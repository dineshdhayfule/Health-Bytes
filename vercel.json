{
    "version": 2,
    "builds": [
        {
            "src": "Frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "Frontend/dist"
            }
        },
        {
            "src": "api/index.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 30
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/health",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/Frontend/dist/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}